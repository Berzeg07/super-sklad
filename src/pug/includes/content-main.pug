                        li
                            span Длина:
                            b 6,0 м
                        li
                            span Ширина:
                            b 2,35 м
                        li
                            span Высота
                            b 2,7 м
                        li
                            span Площадь:
                            b 
                                |3,5 м
                                sup 2
                        li
                            span Объем
                            b 
                                |10 м
